    "commands": [
      {
        "command": "markee.markeeEdit",
        "title": "markee /edit"
      },
      {
        "command": "markee.markeeColor1",
        "title": "markee /color1"
      },
      {
        "command": "markee.markeeColor2",
        "title": "markee /color2"
      },
      {
        "command": "markee.markeeColor3",
