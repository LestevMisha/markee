      },
      {
        "command": "markee.markeeRemove",
        "title": "markee /remove"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "markee.markeeRemove",
          "when": "resourceLangId",
          "group": "navigation"
        },
        {
          "command": "markee.markeeSelect",
          "when": "resourceLangId",
          "group": "navigation"
        },
        {
          "command": "markee.markeeColor1",
